        {
            "@type" : "Relationship", 
            "name" :"Streams", 
            "minMultiplicity" : 0, 
            "maxMultiplicity" : 100,
            "target": "dtmi:io:tributech:stream:tcads;1",
            "displayName" : "Streams",
            "description" : "Contains all the TwinCat ADS streams at the given Source"
